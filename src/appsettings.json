    "UseTls": false,
    "Username": "",
    "Password": "",
    "PathToPem": "",
    "SslProtocol": "Tls12"
  },
  "MongoDbSettings": {
    "ConnectionString": "",
    "DatabaseName": "SensorBase"
  }
}
